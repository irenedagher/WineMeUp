<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FEFEF7"
    tools:context=".ActivitySignUp">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sign up"
        android:textStyle="italic"
        android:textColor="#782222"
        android:textSize="50sp"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="85dp"
        tools:ignore="MissingConstraints"/>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Your name"
        android:textStyle="italic"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="206dp"
        tools:ignore="MissingConstraints"/>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email"
        android:textStyle="italic"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="313dp"
        tools:ignore="MissingConstraints"/>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Password"
        android:textStyle="italic"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="423dp"
        tools:ignore="MissingConstraints"/>

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="I'm already registered. Sign in"
        android:textStyle="italic"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="614dp" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="235dp"
        android:layout_height="40dp"
        android:background="@drawable/button_border_red"
        android:ems="10"
        android:inputType="textPersonName"
        android:text=" Wine Drinker "
        android:gravity="center"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="246dp"
        tools:ignore="MissingConstraints"/>

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="235dp"
        android:layout_height="40dp"
        android:background="@drawable/button_border_red"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPersonName"
        android:text="winedrinker@gmail.com "
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="345dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="235dp"
        android:layout_height="40dp"
        android:background="@drawable/button_border_red"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="455dp"
        tools:ignore="MissingConstraints"/>

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/button_background"
        android:text="Sign up"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="532dp" />

</androidx.constraintlayout.widget.ConstraintLayout>