<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FEFEF7"

    tools:context=".ActivitySignIn">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="205dp"
        android:layout_height="146dp"
        android:gravity="center"
        android:text="Already a wine-drinker? SIGN IN"
        android:textStyle="bold"
        android:textColor="#782222"
        android:textSize="25dp"
        android:textStyle="italic"
        tools:layout_editor_absoluteX="94dp"
        tools:layout_editor_absoluteY="45dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email"
        android:textStyle="italic"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="356dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Password"
        android:textStyle="italic"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="495dp" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="235dp"
        android:layout_height="40dp"
        android:background="@drawable/button_border_red"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPersonName"
        android:text=" Wine Drinker "
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="88dp"
        tools:layout_editor_absoluteY="402dp" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="235dp"
        android:layout_height="40dp"
        android:background="@drawable/button_border_red"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPassword"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="94dp"
        tools:layout_editor_absoluteY="532dp" />

    <Button
        android:id="@+id/button_signup"
        android:layout_width="184dp"
        android:layout_height="51dp"
        android:background="@drawable/button_background"
        android:text="Log in with email"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="115dp"
        tools:layout_editor_absoluteY="623dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="68dp"
        android:layout_height="124dp"
        app:srcCompat="@drawable/logowinemeup"
        tools:layout_editor_absoluteX="135dp"
        tools:layout_editor_absoluteY="200dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="68dp"
        android:layout_height="124dp"
        app:srcCompat="@drawable/logowinemeup"
        tools:layout_editor_absoluteX="203dp"
        tools:layout_editor_absoluteY="200dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="68dp"
        android:layout_height="124dp"
        app:srcCompat="@drawable/logowinemeup"
        tools:layout_editor_absoluteX="271dp"
        tools:layout_editor_absoluteY="200dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="68dp"
        android:layout_height="124dp"
        app:srcCompat="@drawable/logowinemeup"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="200dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="68dp"
        android:layout_height="124dp"
        app:srcCompat="@drawable/logowinemeup"
        tools:layout_editor_absoluteX="347dp"
        tools:layout_editor_absoluteY="200dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="68dp"
        android:layout_height="124dp"
        app:srcCompat="@drawable/logowinemeup"
        tools:layout_editor_absoluteX="-11dp"
        tools:layout_editor_absoluteY="200dp" />
</androidx.constraintlayout.widget.ConstraintLayout>